{
    "version":2,
    "builds":[
        {
            "src":"src/main.ts",
            "use":"@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**", 
                    "prisma/schema.prisma", 
                    "node_modules/@prisma/client/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts"
        }
    ]
}